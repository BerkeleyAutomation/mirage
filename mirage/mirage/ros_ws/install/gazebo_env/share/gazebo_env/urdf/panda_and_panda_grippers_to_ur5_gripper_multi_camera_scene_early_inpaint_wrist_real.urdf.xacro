<robot name="ur5e_scene" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find gazebo_env)/urdf/panda_arm_ur5e_gripper_wrist.xacro"/>
  <xacro:include filename="$(find gazebo_env)/urdf/panda_arm_wrist.xacro"/>
  <xacro:include filename="$(find gazebo_env)/urdf/real_ur5_cam_right_wrist.urdf.xacro" />
  <xacro:include filename="$(find gazebo_env)/urdf/real_panda_cam_right_wrist.urdf.xacro" />
  <xacro:include filename="$(find gazebo_env)/urdf/hand.xacro"/>
  <link name="world" />
  <xacro:panda_with_ur5e_gripper_wrist connected_to="world" xyz="0 0 10"/> 

  <xacro:sensor_d435_ur5_right_wrist parent="world">
  <!-- -0.07231925 -0.02613766 0.0715337 -->
