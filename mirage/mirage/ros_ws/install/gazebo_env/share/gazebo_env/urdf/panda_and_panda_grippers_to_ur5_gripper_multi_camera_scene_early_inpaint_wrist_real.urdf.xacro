  </xacro:sensor_d435_ur5_right_wrist>

  <xacro:sensor_d435_panda_right_wrist parent="world">
    <!-- -0.07525412 0.03308232 0.01456681-->
    <!-- 10.71096828 0.3603744 10.48998283" rpy="0.049 0.630 -2.094-->
    <origin xyz="10.70493656 0.3827488 10.48496566" rpy="0.049 0.630 -2.094" />
  </xacro:sensor_d435_panda_right_wrist>

  <xacro:panda_arm_wrist connected_to="world" xyz="10 0 10"/> 
  <xacro:hand ns="panda" rpy="0 0 ${-pi/4}" connected_to="panda_link8"/>

  <xacro:include filename="$(find gazebo_env)/urdf/panda_arm_no_gripper.xacro"/>
  <xacro:panda_arm_no_gripper connected_to="world" xyz="10 20 10"/>

  <xacro:include filename="$(find gazebo_env)/urdf/real_panda_cam_no_gripper_left.urdf.xacro" />
